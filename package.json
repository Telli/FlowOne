{
  "name": "flowone",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "frontend/*",
    "src"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:studio\" \"npm run dev:voice\" \"npm run dev:configurator\"",
    "dev:backend": "cd backend && C:\\Users\\telli\\AppData\\Local\\Programs\\Python\\Python311\\python.exe -m uvicorn app:app --reload --port 8000",
    "dev:studio": "cd src && npm run dev",
    "dev:voice": "cd frontend/voice-studio && npm run dev",
    "dev:configurator": "cd frontend/agent-configurator && npm run dev",
    "build": "npm run build --workspaces",
    "install-all": "npm install",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "@playwright/test": "^1.48.2"
  }
}